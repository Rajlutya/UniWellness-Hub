<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Counselors</title>
    <link rel="stylesheet" href="/CSS/admin.css">
</head>
<body>
    {{> adminnav}}
    <div class="counselorsList">
        <h1>Counselors</h1>
        <table>
            <thead>
                <tr>
                    <th>Serial No.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Type</th>
                    <th>Degree</th>
                    <th>Experience</th>
                </tr>
            </thead>
            <tbody>
                {{#each counselors}}
                <tr>
                    <td>{{serialNo}}</td>
                    <td><a href="/counselorsprofile/{{this._id}}">{{name}}</a></td>
                    <td>{{email}}</td>
                    <td>{{phone}}</td>
                    <td>{{type}}</td>
                    <td>{{degree}}</td>
                    <td>{{experience}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</body>
</html>
